    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageId>Nololiyt.NCaptchaExtensions.Redis</PackageId>
    <Authors>yueyinqiu5990</Authors>
    <Company>Nololiyt</Company>
    <Product>NCaptchaRedis</Product>
    <Description>An extension library of NCaptcha.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Nololiyt/NCaptchaRedis/wiki</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Nololiyt/NCaptchaRedis.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>NCaptcha</PackageTags>
    <Copyright>Â©2021 Nololiyt</Copyright>
